{
  "name": "crypto-dca-db",
  "version": "0.0.1",
  "description": "Database for crypto-dca",
  "main": "index.js",
  "scripts": {
    "start": "npx sequelize db:migrate && nodemon index.js",
    "test": "mocha --exit ./test/tests/**/*.js",
