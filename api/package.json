  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomlagier/crypto-dca/issues"
  },
  "homepage": "https://github.com/tomlagier/crypto-dca#readme",
  "dependencies": {
    "apollo-server-express": "^1.1.7",
    "bcrypt-nodejs": "0.0.3",
    "bitcoin-core": "^1.2.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "graphql": "^0.11.7",
    "graphql-sequelize": "^5.5.0",
    "sequelize": "^4.20.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "graphql-relay": "^0.5.3",
